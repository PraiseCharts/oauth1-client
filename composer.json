{
    "name": "praisecharts/oauth1-client",
    "description": "PraiseCharts OAuth 1 Client Provider for The PHP League OAuth1-Client package",
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth1",
        "client",
        "authorization",
        "authorisation",
        "aweber"
    ],
    "require": {
        "league/oauth1-client": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5"
    },
    "autoload": {
        "psr-4": {
            "PraiseCharts\\OAuth1\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PraiseCharts\\OAuth1\\Client\\Test\\": "tests/src/"
        }
    }
}
